<% layout('../layouts/boilerplate') -%>

    <h1>all Listings</h1>
    <form action="/listings/new" method="get">
        <button type="submit">Add New listing</button>
    </form>
    <ul>
        <% for(let listing of allListings) {%>
            <li><a href="/listings/<%= listing._id %>">
                    <%= listing.title %>
                </a></li>
            <% } %>
    </ul>